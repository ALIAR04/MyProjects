<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<AnchorPane prefHeight="600.0" prefWidth="1100.0" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1" fx:controller="view.PauseMenu">
    <children>
        <VBox alignment="TOP_CENTER" layoutX="398.0" layoutY="89.0" prefHeight="422.0" prefWidth="302.0" spacing="7.0">
         <children>
            <Label text="PauseMenu">
               <font>
                  <Font name="System Bold" size="30.0" />
               </font>
            </Label>
            <Button mnemonicParsing="false" onMouseClicked="#continueOutPause" text="continue" />
            <Button mnemonicParsing="false" onMouseClicked="#mute" text="mute" />
            <Button mnemonicParsing="false" onMouseClicked="#saveAndExitClicked" text="save and exit" />
            <Button mnemonicParsing="false" onMouseClicked="#exitWithoutSaveClicked" text="exit without save" />
         </children></VBox>
    </children>
</AnchorPane>
