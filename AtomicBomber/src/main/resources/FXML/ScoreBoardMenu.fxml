<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<AnchorPane prefHeight="600.0" prefWidth="1100.0" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1" fx:controller="view.ScoreBoardMenu">
    <children>
        <VBox alignment="TOP_CENTER" layoutX="398.0" layoutY="89.0" prefHeight="422.0" prefWidth="302.0" spacing="7.0">
         <children>
            <Label text="Score Board">
               <font>
                  <Font name="System Bold" size="32.0" />
               </font>
            </Label>
            <Button mnemonicParsing="false" onMouseClicked="#showMaxScore" text="Max Score" />
            <Button mnemonicParsing="false" onMouseClicked="#showKills" text="Kills" />
            <Button mnemonicParsing="false" onMouseClicked="#showScore" text="Score" />
            <Button mnemonicParsing="false" onMouseClicked="#showAccuracy" text="Accuracy" />
            <Button mnemonicParsing="false" onMouseClicked="#backToMainMenuClicked" text="back to Main Menu" />
         </children></VBox>
    </children>
</AnchorPane>
